   {\r\n            name: 'Guitar',\r\n            requirement: 0,\r\n          },\r\n          {\r\n            name: 'Base',\r\n            requirement: 0,\r\n          },\r\n          {\r\n            name: 'Drum',\r\n            requirement: 0,\r\n          },\r\n          {\r\n            name: 'Keyboard',\r\n            requirement: 0,\r\n          },\r\n          \r\n        ],\r\n            }\r\n        },\r\n\r\n    components : {\r\n    },\r\n    \r\n    methods: {\r\n        subtract : function(item){\r\n            if (item.requirement - 1 >= 0){\r\n                item.requirement = item.requirement - 1\r\n            }\r\n        },\r\n\r\n        add : function(item){\r\n            item.requirement = item.requirement + 1\r\n        },\r\n\r\n        createSong : function(){\r\n            axios({\r\n                method : \"POST\",\r\n                url : url,\r\n                headers : setToken(),\r\n                params : {\r\n                    user_id : localStorage.getItem('user'),\r\n                    title : this.title,\r\n                    content: this.content\r\n                }\r\n            })\r\n            .then((response) => {\r\n                response\r\n                this.$router.push({\r\n                    name : \"projects\",\r\n                })\r\n            }).catch((error) => {\r\n                console.log(\"Failed to get retreival\", error.response);\r\n            });\r\n        },\r\n\r\n    },\r\n\r\n    mounted() {\r\n\r\n    }\r\n\r\n    }\r\n</script>\r\n\r\n\r\n<style scoped>\r\n    .btn-container{\r\n        margin-left: auto!important;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}