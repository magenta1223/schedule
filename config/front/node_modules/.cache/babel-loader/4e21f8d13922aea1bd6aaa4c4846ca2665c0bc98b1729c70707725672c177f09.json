arams: {\n          user_id: localStorage.getItem('user'),\n          project_id: this.project.id,\n          selectedSongs: JSON.stringify(this.songs),\n          status: this.status,\n          action: \"fix\"\n        }\n      }).then(response => {\n        // saved modal 띄우기\n        console.log(response);\n      }).catch(error => {\n        console.log(\"Failed to fix the project\", error.response);\n      });\n    }\n  },\n  mounted() {\n    window.onresize = () => {\n      this.windowHeight = window.innerHeight;\n    };\n  },\n  watch: {\n    songs: {\n      handler: function (newVal, oldVal) {\n        newVal;\n        oldVal;\n      },\n      deep: true,\n      immediate: true\n    }\n  }\n};","map":{"version":3,"mappings":";AA+CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,iBAAgB;;AAGrC;AACA,IAAIC,GAAE,GAAI,oCAAoC,EAAG;;AAIjD,eAAe;EAEXC,IAAG,EAAI,eAAe;EAEtBC,IAAG,EAAI,MAAM;IACT,OAAO;MACH;MACAC,YAAW,EAAIC,MAAM,CAACC,WAAW;MACjCC,WAAU,EAAIF,MAAM,CAACG,UAAU;MAE/B;MACAC,MAAK,EAAI,KAAK;MAEd;MACAC,OAAM,EAAI,EAAE;MAEZ;MACAC,WAAU,EAAI,CACV;QAACC,IAAG,EAAI,QAAQ;QAAEC,KAAI,EAAI;MAAe,CAAC,EAC1C;QAACD,IAAG,EAAI,OAAO;QAAEC,KAAI,EAAI;MAAc,CAAC,CAC3C;MACDC,UAAS,EAAI;IACjB;EACJ,CAAC;EAEDC,UAAS,EAAI,CACb,CAAC;EAEDC,OAAO,EAAE;IAELC,eAAc,EAAI,YAAU;MACxBlB,KAAK,CAAC;QACFmB,MAAK,EAAI,KAAK;QACdjB,GAAE,EAAIA,GAAE,GAAI,IAAI,CAACkB,MAAM,CAACC,MAAM,CAACC,UAAS,GAAI,GAAG;QAC/CC,OAAM,EAAItB,QAAQ,EAAE;QACpBoB,MAAK,EAAI;UACLG,OAAM,EAAIC,YAAY,CAACC,OAAO,CAAC,MAAM;QACzC;MACJ,CAAC,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACjB,OAAM,GAAIiB,QAAQ,CAACxB,IAAG;QAC3B,IAAI,CAACyB,MAAK,GAAI,IAAI,CAAClB,OAAO,CAACkB,MAAK;QAChC,IAAIC,IAAG,GAAI,IAAI,CAACC,OAAO,CAAC,IAAI,CAACpB,OAAO,CAACkB,MAAM;QAC3CC,IAAI;QAEJ,IAAI,CAACE,OAAO,CAACC,OAAO,CAAC;UACjB9B,IAAG,EAAI,eAAe;UACtBkB,MAAK,EAAI;YACLQ,MAAK,EAAI,IAAI,CAACA;UAClB;QACJ,CAAC;MACL,CAAC,CAAC,CAACK,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAEF,KAAK,CAACP,QAAQ,CAAC;MAC1D,CAAC,CAAC;IACN,CAAC;IAGDU,EAAC,EAAI,UAASC,WAAW,EAAC;MACtB;;MAEA,IAAI,